<body>
    <div id="app">
        <div class="main-wrapper main-wrapper-1">
            <div class="navbar-bg"></div>


            <nav class="navbar navbar-expand-lg main-navbar">
                <form class="form-inline mr-auto">
                    <ul class="navbar-nav mr-3">
                        <li><a href="#" data-toggle="sidebar" class="nav-link nav-link-lg"><i class="fas fa-bars"></i></a></li>
                    </ul>

                </form>
                <ul class="navbar-nav navbar-right">

                    <li class="dropdown">
                        <a href="#" data-toggle="dropdown" class="nav-link dropdown-toggle nav-link-lg nav-link-user">
                            <img alt="image" src="<%=urlMain %>/img/admin/<%=image %>" class="rounded-circle mr-1">
                            <div class="d-sm-none d-lg-inline-block">salut,<%=name %></div>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right">
                            <a href="<%=urlMain %>/adminProfile" class="dropdown-item has-icon">
                                <i class="far fa-user"></i> Profil
                            </a>
                            <div class="dropdown-divider"></div>
                            <a href="#" data-toggle="modal" data-target="#sign-out" class="dropdown-item has-icon text-danger">
                                <i class="fas fa-sign-out-alt"></i> Se déconnecter
                            </a>
                        </div>
                    </li>
                </ul>
            </nav>














            <div class="main-sidebar sidebar-style-2">
                <aside id="sidebar-wrapper">
                    <div class="sidebar-brand">
                        <a href="<%=urlMain %>/dashboard">Ur-Services24</a>
                    </div>

                    <ul class="sidebar-menu">

                        <li class="dropdown active">
                            <a href="<%=urlMain %>/dashboard" class="nav-link   "><i class="fas fa-fire"></i><span>Tableau de bord</span></a>
                        </li>
                        <li class="dropdown">
                            <a href="<%=urlMain %>/getPlombier" class="nav-link   "><i class="fas fa-wrench"></i><span>Plombier</span></a>

                        </li>
                        <li class="dropdown">
                            <a href="<%=urlMain %>/getElectricien" class="nav-link   "><i class="fas fa-bolt"></i><span>Électricien</span></a>
                        </li>
                        <li class="dropdown">
                            <a href="<%=urlMain %>/getSerrurier" class="nav-link   "><i class="fas fa-lock"></i><span>Serrurier</span></a>
                        </li>
                        <li class="dropdown">
                            <a href="<%=urlMain %>/getservices" class="nav-link   "><i class="fab fa-servicestack"></i><span>services</span></a>
                        </li>

                    </ul>

                </aside>
            </div>
            <div class="main-content" style="min-height: 627px;">
                <section class="section">
                    <div class="section-header">
                        <h1>Tableau de bord</h1>
                    </div>
                    <div class="row">
                        <div class="col-lg-3 col-md-6 col-sm-6 col-12">
                            <div class="card card-statistic-1">
                                <div class="card-icon bg-primary">
                                    <i class="fas fa-user-plus"></i>
                                </div>
                                <div class="card-wrap">
                                    <div class="card-header">
                                        <h4>demander à rejoindre</h4>
                                    </div>
                                    <div class="card-body">
                                        <h6 id="asktojoinClient"></h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-6 col-sm-6 col-12">
                            <div class="card card-statistic-1">
                                <div class="card-icon bg-danger">
                                    <i class="far fa-window-close"></i>
                                </div>
                                <div class="card-wrap">
                                    <div class="card-header">
                                        <h4>Total annulé </h4>
                                    </div>
                                    <div class="card-body">
                                        <h6 id="totaltocancel"></h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-6 col-sm-6 col-12">
                            <div class="card card-statistic-1">
                                <div class="card-icon bg-warning">
                                    <i class="fas fa-award"></i>
                                </div>
                                <div class="card-wrap">
                                    <div class="card-header">
                                        <h4>Service total</h4>
                                    </div>
                                    <div class="card-body">
                                        <h6 id="totaltoservice"></h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-6 col-sm-6 col-12">
                            <div class="card card-statistic-1">
                                <div class="card-icon bg-success">
                                    <i class="fas fa-users-cog"></i>
                                </div>
                                <div class="card-wrap">
                                    <div class="card-header">
                                        <h4>Tous les clients</h4>
                                    </div>
                                    <div class="card-body">
                                        <h6 id="totalclients"></h6>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </section>
                <div class="row">
                    <div class="col-12 col-md-12 col-lg-12">
                        <div class="card">
                            <div class="card-header">
                                <h4>Demande d'adhésion</h4>
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <div id="table-1_wrapper" class="dataTables_wrapper container-fluid dt-bootstrap4 no-footer">

                                        <div class="row">
                                            <div class="col-sm-12">
                                                <table class="table table-striped dataTable no-footer" id="TableClientData" role="grid" aria-describedby="table-1_info">
                                                    <thead>

                                                        <tr role="row">
                                                            <th class=" sorting_asc" tabindex="0" aria-controls="TableClientData" rowspan="1" colspan="1" aria-sort="ascending" aria-label="#: activate to sort column descending" style="width: 36.4px;">
                                                                #
                                                            </th>
                                                            <th class="sorting" tabindex="0" aria-controls="TableClientData" rowspan="1" colspan="1" aria-label="Task Name: activate to sort column ascending" style="width: 182px;">Le métier</th>
                                                            <th class="sorting_disabled" rowspan="1" colspan="1" aria-label="Progress" style="width: 96.8px;">Email</th>
                                                            <th class="sorting_disabled" rowspan="1" colspan="1" aria-label="Members" style="width: 247.2px;">téléphone</th>
                                                            <th class="sorting_disabled" rowspan="1" colspan="1" aria-label="Members" style="width: 300px;"> la zone de travail</th>
                                                            <th class="sorting_disabled" rowspan="1" colspan="1" aria-label="Members" style="width: 247.2px;"> Statut</th>
                                                            <th class="sorting" tabindex="0" aria-controls="TableClientData" rowspan="1" colspan="1" aria-label="Due Date: activate to sort column ascending" style="width: 200px;">Action</th>

                                                        </tr>
                                                    </thead>



                                                </table>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>





            </div>




        </div>



    </div>
    <!--logout Model-->
    <div class="modal" id="sign-out">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title"> Vouloir partir ?</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>
                <div class="modal-body">
                    Appuyez sur déconnexion pour quitter
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-success" data-dismiss="modal">
                        Reste ici
                      </button>
                    <a href="<%=urlMain %>/logout" class="btn btn-danger"> Se déconnecter</a>
                </div>
            </div>
        </div>

    </div>

    <!--start Model 2-->
    <div class="modal fade" id="ActionAcceptClient" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Accept Or Reject Clients</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
                </div>
                <form id="FormUpdateClientStatus">
                    <div class="modal-body">
                        <select id="Select_Update_Status" class="custom-select">
                
                <option value="Nouvelle_cliente">Nouvelle cliente</option>
                <option value="J'accepte">J'accepte</option>
                <option value="refuser">refuser</option>
              
               
            </select>
                    </div>

                    <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Fermer</button>
                        <input type="submit" class="btn btn-primary" value="Sauvegarder les modifications">
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!--End Model-->
    <!--start Delete Model-->
    <div class="modal fade" tabindex="-1" role="dialog" id="ActionDeleteClient">
        <div class="modal-dialog modal-md" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Are You Sure?</h5> <button type="button" class="close" data-dismiss="modal" aria-label="Close"> 
                                      <span aria-hidden="true">×</span>             </button> </div>
                <form id="FormRemoveClient">
                    <div class="modal-body"> This action can not be undone. Do you want to continue?</div>
                    <div class="modal-footer">
                        <input type="submit" class="btn btn-danger btn-shadow" value="Yes">
                        <button type="button" class="btn btn-secondary" id="" data-dismiss="modal" aria-label="Close">Cancel</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!--End Delete Model-->


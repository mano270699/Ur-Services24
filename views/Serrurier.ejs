<body>
    <div id="app">
        <div class="main-wrapper main-wrapper-1">
            <div class="navbar-bg"></div>


            <nav class="navbar navbar-expand-lg main-navbar">
                <form class="form-inline mr-auto">
                    <ul class="navbar-nav mr-3">
                        <li><a href="#" data-toggle="sidebar" class="nav-link nav-link-lg"><i class="fas fa-bars"></i></a></li>
                    </ul>

                </form>
                <ul class="navbar-nav navbar-right">

                    <li class="dropdown">
                        <a href="#" data-toggle="dropdown" class="nav-link dropdown-toggle nav-link-lg nav-link-user">
                            <img alt="image" src="<%=urlMain %>/img/admin/<%=image %>" class="rounded-circle mr-1">
                            <div class="d-sm-none d-lg-inline-block">salut, <%=name %></div>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right">
                            <a href="<%=urlMain %>/adminProfile" class="dropdown-item has-icon">
                                <i class="far fa-user"></i> Profil
                            </a>
                            <div class="dropdown-divider"></div>
                            <a href="#" data-toggle="modal" data-target="#sign-out" class="dropdown-item has-icon text-danger">
                                <i class="fas fa-sign-out-alt"></i> Se déconnecter
                            </a>
                        </div>
                    </li>
                </ul>
            </nav>














            <div class="main-sidebar sidebar-style-2">
                <aside id="sidebar-wrapper">
                    <div class="sidebar-brand">
                        <a href="/dashboard">Ur-Services24</a>
                    </div>

                    <ul class="sidebar-menu">

                        <li class="dropdown ">
                            <a href="/dashboard" class="nav-link   "><i class="fas fa-fire"></i><span>Tableau de bord</span></a>
                        </li>
                        <li class="dropdown ">
                            <a href="/getPlombier" class="nav-link   "><i class="fas fa-wrench"></i><span>Plombier</span></a>

                        </li>
                        <li class="dropdown ">
                            <a href="/getElectricien" class="nav-link   "><i class="fas fa-bolt"></i><span>Électricien</span></a>
                        </li>
                        <li class="dropdown active">
                            <a href="/getSerrurier" class="nav-link   "><i class="fas fa-lock"></i><span>Serrurier</span></a>
                        </li>
                        <li class="dropdown">
                            <a href="/getservices" class="nav-link   "><i class="fab fa-servicestack"></i><span>services</span></a>
                        </li>

                    </ul>

                </aside>
            </div>
            <div class="main-content" style="min-height: 627px;">
                <section class="section">
                    <div class="section-header">
                        <h1>Serrurier</h1>
                    </div>


                </section>
                <div class="row">
                    <div class="col-12 col-md-12 col-lg-12 mt-5">
                        <div class="card">
                            <div class="card-header">
                                <h4>Clients de Serrurier</h4>
                            </div>
                            <div class="card-body">
                                <div class="table-responsive">
                                    <div id="table-1_wrapper" class="dataTables_wrapper container-fluid dt-bootstrap4 no-footer">

                                        <div class="row">
                                            <div class="col-sm-12">
                                                <table class="table table-striped dataTable no-footer" id="tableSerrurier" role="grid" aria-describedby="table-1_info">
                                                    <thead>

                                                        <tr role="row">
                                                            <th class=" sorting_asc" tabindex="0" aria-controls="tableSerrurier" rowspan="1" colspan="1" aria-sort="ascending" aria-label="#: activate to sort column descending" style="width: 36.4px;">
                                                                #
                                                            </th>
                                                            <th class="sorting" tabindex="0" aria-controls="tableSerrurier" rowspan="1" colspan="1" aria-label="Task Name: activate to sort column ascending" style="width: 182px;">Le métier</th>
                                                            <th class="sorting_disabled" rowspan="1" colspan="1" aria-label="Progress" style="width: 96.8px;">Email</th>
                                                            <th class="sorting_disabled" rowspan="1" colspan="1" aria-label="Members" style="width: 247.2px;">téléphone</th>
                                                            <th class="sorting_disabled" rowspan="1" colspan="1" aria-label="Members" style="width: 247.2px;"> la zone de travail</th>
                                                            <th class="sorting" tabindex="0" aria-controls="tableSerrurier" rowspan="1" colspan="1" aria-label="Due Date: activate to sort column ascending" style="width: 111.6px;">Action</th>
                                                            <th class="sorting" tabindex="0" aria-controls="table-1" rowspan="1" colspan="1" aria-label="Due Date: activate to sort column ascending" style="width: 111.6px;">Profil</th>


                                                        </tr>
                                                    </thead>


                                                </table>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>





            </div>




        </div>



    </div>
    <!--logout Model-->
    <div class="modal" id="sign-out">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title"> Vouloir partir ?</h4>
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                </div>
                <div class="modal-body">
                    Appuyez sur déconnexion pour quitter
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-success" data-dismiss="modal">
                        Reste ici
                      </button>
                    <a href="login.html" class="btn btn-danger"> Se déconnecter</a>
                </div>
            </div>
        </div>

    </div>

    <!--Add Service to client 3-->
    <div class="modal fade" id="ActionserviceSerrurier" tabindex="-1" role="dialog" aria-labelledby="UpdateMealTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-scrollable" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="UpdateMealTitle">
                        <p class="textstayle">Add service to Client</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
              </button>
                </div>
                <div class="modal-body">

                    <form id="FormserviceSerrurier">
                        <div class="form-group">
                            <label for="recipient-name" class="col-form-label">Nom de la tâche:</label>
                            <input type="text" id="TaskName" class="form-control" required>
                        </div>
                        <div class="form-group">
                            <label for="recipient-name" class="col-form-label">Nom de la customer:</label>
                            <input type="text" id="cutomer_name" class="form-control" required>
                        </div>


                        <div class="form-group">
                            <label for="recipient-name" class="col-form-label">Téléphone du client:</label>
                            <input type="tel" id="cust-phone" class="form-control" required>
                        </div>
                        <div class="form-group">
                            <label for="recipient-name" class="col-form-label">Le coût du service:</label>
                            <div class="input-group mb-2">
                                <div class="input-group-prepend">
                                    <span class="input-group-text">€</span>

                                </div>
                                <input type="text" id="Service_price" required class="form-control" aria-label="Euro amount (with dot and two decimal places)">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="message-text" class="col-form-label text-lg">Adresse du client</label>
                            <input type="text" id="CustomerAddress" class="form-control" required>
                        </div>
                        <div class="form-group">
                            <label for="message-text" class="col-form-label">La description:</label>
                            <textarea class="form-control" id="Service_Desc"> </textarea>
                        </div>

                        <div class="modal-footer">
                            <input type="submit" class="btn btn-primary " value="ajouter un service">
                            <button type="button" class="btn btn-secondary ml-auto text-dark" data-dismiss="modal">Annuler</button>

                        </div>
                    </form>

                </div>

            </div>
        </div>
    </div>

    <!--End Model 3-->

